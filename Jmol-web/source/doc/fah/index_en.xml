<?xml version="1.0"?>
<!DOCTYPE somethingOrAnything [
<!ENTITY projects SYSTEM "fah-projects.xml" >
]>
<document>
  <body>
    <title>Jmol Applet for Folding@home</title>
    <section>

    <form name="fahForm" action="">
      <script src="./fah.js" type="text/javascript"></script>
      <script src="./createProjectList.js" type="text/javascript"></script>
      <table border="0" cellpadding="0" cellspacing="0" width="100%">
      <tr><td>
        <table border="0" cellpadding="0" cellspacing="0" width="100%"><tr>
          <td>
            <script type="text/javascript">jmolApplet(350, "", "Fah");</script>
          </td>
          <td valign="top">
            <script type="text/javascript">
              addRowStanfordLogo();
              addRowProjectNumber("Project");
              addRowProjectName("Project Name");
              addRowProjectCredit("Credit");
              addRowProjectAtoms("Number of atoms");
              addRowProjectPreferred("Preferred");
              addRowProjectDeadline("Final deadline");
              addRowProjectFrames("Frames");
              addRowProjectCode("Code");
              addRowButtons(
                "Project description",
                "Hide solvent",
                "<fah_count_files/> files / <fah_count_projects/> projects.");
              addRowCurrentXyz(
                "If you have &lt;b&gt;current.xyz&lt;/b&gt; files for the missing projects, please send them at nicove@users.sourceforge.net");
            </script>
          </td>
        </tr></table>
      </td></tr>

      <tr>
        <td class="btfah">
          <script type="text/javascript">
            &projects;
          </script>
        </td>
      </tr>
      </table>
    </form></section>
  </body>
</document>
