# hansonr@stolaf.edu

topic = "biophysics"
NIOcontentScript=topic + "/" + topic + ".spt"

function loadModel(jmolFile,more,banner) {
  if (!jmolFile) { return }
  zap
  var msg = "banner:" + banner
  message @msg
  cd ""
  cd @topic
  load @jmolFile
  if (more) { script inline @more }
  delay 30
}