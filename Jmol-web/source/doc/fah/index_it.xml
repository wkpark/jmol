<!DOCTYPE somethingOrAnything [
<!ENTITY projects SYSTEM "fah-projects.xml" >
]>
<document>
  <body>
    <title>Applet Jmol per Folding@home</title>
    <section>

    <form name="fahForm">
      <table border="1">
      <tr>
        <table border="0"><tr>
          <td>
            <script>jmolApplet(350, "", "Fah");</script>
          </td>
          <td valign="top">
            <table border="0" cellpadding="0" cellspacing="0">
              <tr>
                <td align="center" colspan="2">
                  <a href="http://folding.stanford.edu">
                    <img src="FAHlogoButton.jpg" alt="fah-logo" border="0"/>
                  </a>
                </td>
              </tr>
              <tr>
                <td><label>Progetto:</label></td>
                <td><input type='text' name='infoNumber' id='infoNumber' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Nome:</label></td>
                <td><input type='text' name='infoName' id='infoName' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Punti:</label></td>
                <td><input type='text' name='infoCredit' id='infoCredit' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Numero di atomi:</label></td>
                <td><input type='text' name='infoAtoms' id='infoAtoms' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Termine consigliato:</label></td>
                <td><input type='text' name='infoPreferred' id='infoPreferred' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Termine ultimo:</label></td>
                <td><input type='text' name='infoDeadline' id='infoDeadline' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Frames:</label></td>
                <td><input type='text' name='infoFrames' id='infoFrames' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td><label>Codice:</label></td>
                <td><input type='text' name='infoCode' id='infoCode' size='50' readonly='readonly' /></td>
              </tr>
              <tr>
                <td align="left" colspan="2">
                  <br/>
                  <input type='button' value='Descrizione progetto'
                    onclick='showProjectDescription()'/>
                  <input type='button' value='XML'
                    onclick='showXMLFile()'/>
                  <small>
                    <fah_count_files/> file / <fah_count_projects/> progetti.
                  </small>
                </td>
              </tr>
              <tr>
                <td align="left" colspan="2">
                  <small><i>
                    Se disponete di file .xyz dei progetti mancanti, inviateli a nicove@users.sourceforge.net, grazie
                  </i></small>
                </td>
              </tr>
            </table>
          </td>
        </tr></table>
      </tr>

      <tr>
        <td class="btfah">
          <script>
            &projects;
          </script>
        </td>
      </tr>
      </table>
    </form></section>
  </body>
</document>
